{
  "private": true,
  "workspaces": [
    "packages/*",
    "client/*",
    "server/*"
  ],
  "dependencies": {
    "concurrently": "^5.1.0",
    "dotenv-cli": "^3.1.0",
    "knex": "<0.20.0",
    "node-fetch": "^3.1.1",
    "pg": "latest",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "scripts": {
    "dev:webapp": "dotenv concurrently  \"yarn workspace client-web dev -p 80\"",
