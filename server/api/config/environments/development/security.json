{
  "csp": {
    "enabled": true,
    "policy": [
      "block-all-mixed-content"
    ]
  },
  "p3p": {
    "enabled": false,
    "value": ""
  },
  "hsts": {
    "enabled": true,
    "maxAge": 31536000,
    "includeSubDomains": true
  },
  "xframe": {
    "enabled": true,
    "value": "SAMEORIGIN"
  },
  "xss": {
    "enabled": true,
    "mode": "block"
  },
  "cors": {
    "enabled": true,
    "origin": "${process.env.APP_PROTOCOL}${process.env.PROXY_HOST}${process.env.PROXY_PORT !== '80'?`:${process.env.PROXY_PORT}`: ''}, ${process.env.APP_PROTOCOL}${process.env.API_HOST}${process.env.API_PORT !== '80'?`:${process.env.API_PORT}`: ''}, ${process.env.APP_PROTOCOL}${process.env.WEBAPP_HOST}${process.env.WEBAPP_PORT !== '80'?`:${process.env.WEBAPP_PORT}`: ''}",
    "credentials": true
  },
  "ip": {
    "enabled": false,
    "whiteList": [],
    "blackList": []
  }
}